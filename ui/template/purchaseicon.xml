<UIButton templateName="purchaseicon" name="icon" toggle="0" sticky="1" broadcast="1">
   <UIRegion w="57" h="48"/>
   <UIAspect alwayshit="region">
      <standard>
         <normal>
            <BFRect x="3" y="5" w="57" h="48"/>
         </normal>
         <highlighted>
            <BFRect x="3" y="69" w="57" h="48"/>
         </highlighted>
         <activated>
            <BFRect x="67" y="5" w="57" h="48"/>
         </activated>
         <disabled>
            <BFRect x="67" y="69" w="57" h="48"/>
         </disabled>
      </standard>
      <alternate>
         <normal>
            <BFRect x="67" y="5" w="57" h="48"/>
         </normal>
         <highlighted>
            <BFRect x="67" y="5" w="57" h="48"/>
         </highlighted>
         <activated>
            <BFRect x="67" y="5" w="57" h="48"/>
         </activated>
         <disabled>
            <BFRect x="67" y="69" w="57" h="48"/>
         </disabled>
      </alternate>
   </UIAspect>
   <children>
      <UIMultiIcon name="product_icon">
         <UIRegion x="0" y="0" w="20" h="20" xalign="min" yalign="min" walign="len" halign="len"/>
         <UIState visible="normal"/>
         <UIAspect alwayshit="never">
            <default image="">
               <BFColor r="255" g="255" b="255" a="184"/>
            </default>
         </UIAspect>
         <images>
            <image key="Zoo" image=""/>
            <image key="EndangeredSpecies" image="ui\shared\leaf_icon.dds">
               <BFRect x="0" y="0" w="16" h="16"/>
            </image>
            <image key="PDLC1" image="ui\shared\dino_icon.dds">
               <BFRect x="0" y="0" w="16" h="16"/>
            </image>
            <image key="CP1" image="ui\shared\africa_icon.dds">
               <BFRect x="0" y="0" w="16" h="16"/>
            </image>
            <image key="Marine" image="ui\shared\marine_icon.dds">
               <BFRect x="0" y="0" w="16" h="16"/>
            </image>              
            <image key="Extinct" image="ui\shared\extinct_icon.dds">
               <BFRect x="0" y="0" w="16" h="16"/>
            </image>              
         </images>
      </UIMultiIcon>
      <UIMultiIcon name="show_icon">
         <UIRegion x="0" y="0" w="20" h="20" xalign="min" yalign="min" walign="len" halign="len"/>
         <UIState visible="normal"/>
         <UIAspect alwayshit="never">
            <default image="">
               <BFColor r="255" g="255" b="255" a="184"/>
            </default>
         </UIAspect>
         <images>
            <image key="MarineShow" image="ui\shared\MarineMania_showanimal_icon.dds">
               <BFRect x="0" y="0" w="16" h="16"/>
            </image>              
         </images>
      </UIMultiIcon>      
   </children>
</UIButton>
