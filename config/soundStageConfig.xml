<BFSoundStageMgr>
   <SoundStage name="default">
      <data roomType="ENVIRONMENT_FOREST" roomEffectLevel=".2" globalOcclusionLevel="0.0" globalExclusionLevel="0.0" globalObstructionlevel="0.0"/>
   </SoundStage>
   <!-- The main reason for "GeorgeTest" is to provide the only sound stage that actually can have it's values altered! -->
   <SoundStage name="GeorgeTest">
      <data roomType="ENVIRONMENT_UNDERWATER" roomEffectLevel="1.0" globalOcclusionLevel="0.0" globalExclusionLevel="0.0" globalObstructionlevel="0.0"/>
   </SoundStage>
   <SoundStage name="underwater_natural_small">
      <data roomType="ENVIRONMENT_UNDERWATER" roomEffectLevel="0.7" globalOcclusionLevel="0.9" globalExclusionLevel="0.5" globalObstructionlevel="0.5" ambientSound="tank_small_natural_loop" soundFilterLayer="underwater"/>
      <ambients>
         <snds>
            <snd name="a_tank_natural_small"/>
         </snds>
      </ambients>
   </SoundStage>
   <SoundStage name="underwater_natural_medium">
      <data roomType="ENVIRONMENT_UNDERWATER" roomEffectLevel="0.7" globalOcclusionLevel="0.9" globalExclusionLevel="0.5" globalObstructionlevel="0.5" ambientSound="tank_med_natural_loop" soundFilterLayer="underwater"/>
      <ambients>
         <snds>
            <snd name="a_tank_natural_med"/>
         </snds>
      </ambients>
   </SoundStage>
   <SoundStage name="underwater_natural_large">
      <data roomType="ENVIRONMENT_UNDERWATER" roomEffectLevel="0.7" globalOcclusionLevel="0.9" globalExclusionLevel="0.5" globalObstructionlevel="0.5" ambientSound="tank_large_natural_loop" soundFilterLayer="underwater"/>
      <ambients>
         <snds>
            <snd name="a_tank_natural_large"/>
         </snds>
      </ambients>
   </SoundStage>
   <SoundStage name="underwater_tank_small">
      <data roomType="ENVIRONMENT_UNDERWATER" roomEffectLevel="0.8" globalOcclusionLevel="0.9" globalExclusionLevel="0.5" globalObstructionlevel="0.5" ambientSound="tank_small_loop" soundFilterLayer="underwater"/>
      <ambients>
         <snds>
            <snd name="a_tank_small"/>
         </snds>
      </ambients>
   </SoundStage>
   <SoundStage name="underwater_tank_medium">
      <data roomType="ENVIRONMENT_UNDERWATER" roomEffectLevel="0.8" globalOcclusionLevel="0.9" globalExclusionLevel="0.5" globalObstructionlevel="0.5" ambientSound="tank_med_loop" soundFilterLayer="underwater"/>
      <ambients>
         <snds>
            <snd name="a_tank_med"/>
         </snds>
      </ambients>
   </SoundStage>
   <SoundStage name="underwater_tank_large">
      <data roomType="ENVIRONMENT_UNDERWATER" roomEffectLevel="0.8" globalOcclusionLevel="0.9" globalExclusionLevel="0.5" globalObstructionlevel="0.5" ambientSound="tank_large_loop" soundFilterLayer="underwater"/>
      <ambients>
         <snds>
            <snd name="a_tank_large"/>
         </snds>
      </ambients>
   </SoundStage>
   <SoundStage name="whalehall">
      <data roomType="ENVIRONMENT_HALLWAY" roomEffectLevel="0.9" globalOcclusionLevel="0.4" globalExclusionLevel="0.2" globalObstructionlevel="0.2" ambientSound="ambient_whalehall" useIndoorOcclusion="true"/>
   </SoundStage>
   <SoundStage name="redwoodtunnel">
      <data roomType="ENVIRONMENT_CARPETEDHALLWAY" roomEffectLevel="0.8" globalOcclusionLevel="0.4" globalExclusionLevel="0.4" globalObstructionlevel="0.4" dontChangeAmbientSounds="true"/>
   </SoundStage>
   <SoundStage name="crystaltunnel">
      <data roomType="ENVIRONMENT_BATHROOM" roomEffectLevel="0.8" globalOcclusionLevel="0.9" globalExclusionLevel="0.0" globalObstructionlevel="0.0" dontChangeAmbientSounds="true"/>
   </SoundStage>
   <SoundStage name="vehiclestation">
      <data roomType="ENVIRONMENT_HANGAR" roomEffectLevel="0.3" globalOcclusionLevel="0.3" globalExclusionLevel="0.0" globalObstructionlevel="0.0" dontChangeAmbientSounds="true"/>
   </SoundStage>
   
   <SoundStage name="cloning_center">
      <data roomType="ENVIRONMENT_STONEROOM" roomEffectLevel="0.9" globalOcclusionLevel="0" globalExclusionLevel="0" globalObstructionlevel="0.2" ambientSound="cloningcenter_ambience" useIndoorOcclusion="true"/>
      <ambients>
         <snds>
            <snd name="cloningcenter_oneshots"/>
         </snds>
      </ambients>
   </SoundStage>   
   <SoundStage name="cavepaintinghall">
      <data roomType="ENVIRONMENT_CAVE" roomEffectLevel="0.2" globalOcclusionLevel="0.4" globalExclusionLevel="0.2" globalObstructionlevel="0.2" ambientSound="ambient_cavepaintinghall" useIndoorOcclusion="true"/>
   </SoundStage>
   
