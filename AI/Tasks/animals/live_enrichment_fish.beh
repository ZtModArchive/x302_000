<BehaviorSets>
   <Idle>
      <subjects>
         <live_enrichment_fish/>
         <live_scenery_fish/>
      </subjects>
      <behaviors>
         <BFBehAnimateSwitch>
            <animTable>
               <Walk anim="Swim_Ahead"/>
               <Run anim="Swim_Ahead"/>
               <Stand anim="Swim_Ahead"/>
               <default anim="Swim_Ahead"/>
            </animTable>
         </BFBehAnimateSwitch>
      </behaviors>
   </Idle>
   <Stand>
      <subjects>
         <live_scenery_fish/>
      </subjects>
      <behaviors>
         <BFBehAnimate targetAnim="TreadWater_Idle"/>
      </behaviors>
   </Stand>
   <Swim_Idle>
      <subjects>
         <live_enrichment_fish/>
      </subjects>
      <behaviors>
         <BFBehAnimate targetAnim="Swim_Ahead"/>
      </behaviors>
   </Swim_Idle>
   <DockAttacked>
      <subjects>
         <live_enrichment_fish/>
         <live_scenery_fish/>
      </subjects>
      <behaviors>
         <BFBehScript context="behavior" file="scripts/playparticle.lua" function="playParticle" params="{Floor} runps PlacementCloud"/>
         <BFBehDockNow targetNode="p_PredatorOffset" subjectNode="p_PreyOffset" reserveSlotName="general"/>
      </behaviors>
   </DockAttacked>
   <Attacked>
      <subjects>
         <live_enrichment_fish/>
         <live_scenery_fish/>
      </subjects>
      <behaviors>
         <BFBehAnimate targetAnim="Swim_Turn180" loopFlag="true"/>
      </behaviors>
   </Attacked>
   <FleeMarinePredator>
      <subjects>
         <live_enrichment_fish/>
         <live_scenery_fish/>
      </subjects>
      <behaviors>
         <BFBehEvasion evasionRadius="30" locoSpeed="fast" avoidLand="true"/>
      </behaviors>
   </FleeMarinePredator>
   <ChasePreyWater>
      <subjects>
         <Felidae/>
         <CrocodileNile/>
