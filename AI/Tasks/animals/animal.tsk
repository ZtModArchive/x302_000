   <BFAITaskTemplate Name="HungryLargeAnimal" TaskDelayMin="30" TaskDelayMax="60" UniqueID="animal:HungryLargeAnimal">
      <BFAICreateData>
         <Subjects>
            <b_LargeAnimal/>
            <Qualifiers_AND inWater_OR_onLand="true" hunger="GE 90"/>
         </Subjects>
         <Targets>
            <self/>
         </Targets>
         <Objects/>
      </BFAICreateData>
      <BFAIEvalData fixedScore="1000"/>
      <BFBehExecTask>
         <ZTBehViewEvent viewKey="Neg_5">
            <ZTFeedbackData>
               <ZTThoughtInfo locID="guestthoughts:AnimalHungryBad" priority="-1" timeout="60" global="false" useTargetName="true"/>
            </ZTFeedbackData>
         </ZTBehViewEvent>
         <ZTBehFeedback>
            <ZTFeedbackData>
               <ZTEmoticonInfo emoticonName="EmoteAnimalHungerBad"/>
               <ZTActionInfo locID="animalactions:LookingForFood"/>
               <ZTThoughtInfo locID="animalthoughts:HungryAnimal" priority="1" timeout="30" global="true"/>
               <ZTMessageInfo locID="zoomessages:CriticalHungryAnimal" priority="-1" timeout="15" interval="60" global="true" useEntityName="true" filterAttribute="b_Rampage EQ true"/>
            </ZTFeedbackData>
         </ZTBehFeedback>
      </BFBehExecTask>
      <BFAICompletionData>
         <BFAIAttributeFloatMap health="5"/>
      </BFAICompletionData>
   </BFAITaskTemplate>
