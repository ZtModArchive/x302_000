<BFAITaskTemplateList>
<!-- Disease Effects-->
   <BFAITaskTemplate Name="ReceiveWanderlustDisease" TaskDelayMin="600" TaskDelayMax="1200" Priority="3"  UniqueID="DiseaseWanderlust:ReceiveWanderlustDisease">
      <BFAICreateData>
         <Subjects>
            <animal/>
         </Subjects>
         <Targets>
            <self/>
         </Targets>
         <Objects>
            <t_GetWanderlustDisease/>
         </Objects>
      </BFAICreateData>
      <BFAIEvalData fixedScore="500"/>
      <BFBehExecTask>
         <BFBehScript context="behavior" file="scenario/scripts/disease.lua" function="makeAnimalSick" params="Wanderlust"/>
         <BFBehSendToken>
	    <BFAITokenList>
	       <BFAIToken Name="t_WanderlustDisease" GiveTo="subject" Timein="0" Timeout="120" Reconsider="true"/>
	    </BFAITokenList>
         </BFBehSendToken>
      </BFBehExecTask>
      <BFAICompletionData/>
      <BFAIFailureData>
         <BFBehExecTask>
            <BFBehPlaySet behSet="FailureSet"/>
         </BFBehExecTask>
      </BFAIFailureData>
   </BFAITaskTemplate>
