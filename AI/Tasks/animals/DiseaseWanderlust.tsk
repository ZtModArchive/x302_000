   <BFAITaskTemplate Name="GetSick_WanderlustDisease" TaskDelayMin="30" TaskDelayMax="60" Priority="3" UniqueID="DiseaseWanderlust:GetSick_WanderlustDisease">
      <BFAICreateData>
         <Subjects>
            <animal/>
            <Qualifiers b_Disease="true"/>
         </Subjects>
         <Targets>
            <self/>
         </Targets>
         <Objects>
            <t_WanderlustDisease/>
         </Objects>
      </BFAICreateData>
      <BFAIEvalData fixedScore="504"/>
      <BFBehExecTask>
         <ZTBehFeedback>
            <ZTFeedbackData>
               <ZTEmoticonInfo emoticonName="EmoteAnimalHealthBad"/>
               <ZTActionInfo locID="animalactions:Sick"/>
               <ZTMessageInfo locID="animalthoughts:DiseasedAnimal" priority="-1" timeout="60" global="true" useEntityName="true"/>
            </ZTFeedbackData>
         </ZTBehFeedback>
      </BFBehExecTask>
      <BFAICompletionData>
	 <BFAITokenList>
	    <BFAIToken Name="t_WanderlustDisease2" GiveTo="subject" Timein="30" Timeout="90" Chance="100" Reconsider="true"/>
	    <BFAIToken Name="t_WanderlustEscape" GiveTo="subject" Timeout="90" Chance="100"/>
	 </BFAITokenList>
         <BFBehExecTask>
            <BFBehPlaySet behSet="GetSickWanderlust"/>
            <BFBehPlaySet behSet="Sick"/>
            <ZTBehViewEvent viewKey="Neg_8">
               <ZTFeedbackData>
                  <ZTThoughtInfo locID="guestthoughts:AnimalSickBad" priority="-1" timeout="60" global="false" useTargetName="true"/>
               </ZTFeedbackData>
            </ZTBehViewEvent>
            <ZTBehFeedback>
	       <ZTFeedbackData>
	          <ZTActionInfo locID="animalactions:WanderAimless"/>
	       </ZTFeedbackData>
            </ZTBehFeedback>
            <BFBehWander playTime="10"/>
         </BFBehExecTask>
      </BFAICompletionData>
      <BFAIFailureData>
         <BFBehExecTask>
            <BFBehPlaySet behSet="FailureSet"/>
         </BFBehExecTask>
      </BFAIFailureData>
   </BFAITaskTemplate>
